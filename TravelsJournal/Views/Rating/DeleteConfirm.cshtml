@model TravelsJournal.Models.RatingDto

@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>Are you sure you want to delete this Rating?</h2>
<a href="/Rating/List">Cancel</a>
<h3>Rating ID: @Model.RatingID</h3>
<p>Rating Description: @Model.RatingDescription</p>
<form action="/Rating/Delete/@Model.RatingID" method="POST">
    <input type="submit" value="Confirm Delete" />
</form>
